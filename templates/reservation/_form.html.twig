{{ form_start(form) }}
    <!-- Champ pour la date de début -->
    <div>
        {{ form_label(form.created_at) }}
        {{ form_widget(form.created_at) }}
        {{ form_errors(form.created_at) }}
    </div>

    <!-- Champ pour la date de fin -->
    <div>
        {{ form_label(form.ended_at) }}
        {{ form_widget(form.ended_at) }}
        {{ form_errors(form.ended_at) }}
    </div>

    <!-- Autres champs (par exemple, vehicule s'il est affiché) -->
    {% for child in form %}
        {% if child.vars.name not in ['created_at', 'ended_at'] %}
            <div>
                {{ form_label(child) }}
                {{ form_widget(child) }}
                {{ form_errors(child) }}
            </div>
        {% endif %}
    {% endfor %}

    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
